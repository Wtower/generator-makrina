{
  "name": "<%= name %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "repository": {
    "type": "git",
    "url": "git+<%= git %>.git"
  },
  "author": "<%= author %>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "<%= git %>/issues"
  },
  "homepage": "<%= git %>#README",
  "private": true,
  "scripts": {
    "prestart": "npm install",
    "start": "node ./bin/www",
    "test": "gulp test",
    "test-karma": "node_modules/karma-cli/bin/karma start karma.conf.js",
    "test-karma-single-run": "node_modules/karma-cli/bin/karma start karma.conf.js --single-run",
    "update-webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "node_modules/protractor/bin/protractor e2e-tests/protractor.conf.js",
    "deploy": "cd ~/workspace/ansible/ansible-play/books/ && ansible-playbook node_deploy.yml -e \"host=<%= deployHost %> project=<%= name %>\" --ask-vault-pass",
    "server_upgrade": "cd ~/workspace/ansible/ansible-play/books/ && ansible remotes -m apt -a \"upgrade=yes\" -b -K"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-resource": "^1.5.7",
    "angular-route": "^1.5.7",
    "angular-sanitize": "^1.5.7",
    "bluebird": "^3.4.1",
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.6",
    "connect-mongo": "^1.2.1",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "^2.3.0",
    "ejs": "~2.5.1",
    "express": "~4.14.0",
    "express-session": "^1.14.0",
    "font-awesome": "^4.6.3",
    "gentelella": "^1.3.0",
    "i18n": "^0.8.3",
    "jquery": "^2.2.4",
    "mongoose": "^4.5.3",
    "morgan": "~1.7.0",
    "multer": "^1.1.0",
    "newrelic": "^1.28.2",
    "ng-file-upload": "^12.0.4",
    "ng-gentelella": "^0.2.0",
    "nodemailer": "^2.5.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.7",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.7",
    "gulpfile-ninecms": "^0.3.0",
    "nodemailer-stub-transport": "^1.0.0",
    "protractor": "^4.0.2",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.5",
    "supertest": "^2.0.0",
    "yargs": "^6.3.0"
  }
}
