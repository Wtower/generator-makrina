{
  "name": "<%= name %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "repository": {
    "type": "git",
    "url": "git+<%= git %>.git"
  },
  "author": "<%= author %>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "<%= git %>/issues"
  },
  "homepage": "<%= git %>#README",
  "private": true,
  "scripts": {
    "prestart": "npm install",
    "start": "node ./bin/www",
    "test": "node_modules/karma-cli/bin/karma start karma.conf.js",
    "test-single-run": "node_modules/karma-cli/bin/karma start karma.conf.js --single-run",
    "update-webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "node_modules/protractor/bin/protractor e2e-tests/protractor.conf.js",
    "deploy": "cd ~/workspace/ansible/ansible-play/books/ && ansible-playbook node_deploy.yml -e \"host=<%= deployHost %> project=<%= name %>\" --ask-vault-pass"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-resource": "^1.5.7",
    "angular-route": "^1.5.7",
    "bluebird": "^3.4.1",
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.6",
    "connect-mongo": "^1.2.1",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "express": "~4.14.0",
    "express-session": "^1.14.0",
    "font-awesome": "^4.6.3",
    "gentelella": "^1.3.0",
    "i18n": "^0.8.3",
    "jquery": "^2.2.4",
    "mongoose": "^4.5.3",
    "morgan": "~1.7.0",
    "multer": "^1.1.0",
    "newrelic": "^1.28.2",
    "ng-file-upload": "^12.0.4",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.7",
    "gulpfile-ninecms": "^0.1.5",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "protractor": "^3.3.0"
  }
}
